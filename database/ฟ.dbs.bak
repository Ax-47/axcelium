		<script name="clear_user.sql" id="fe8aaab4-abcf-4c39-aa4d-9b4b8f78772c" language="SQL" >
			<string><![CDATA[-- TRUNCATE axcelium.users;
-- TRUNCATE axcelium.user_count_by_app;
-- TRUNCATE axcelium.refresh_tokens;
--DROP TABLE IF EXISTS refresh_tokens;
CREATE MATERIALIZED VIEW refresh_tokens_by_user AS
SELECT token_id, organization_id, application_id, user_id,
       encrypted_token_secret, token_version, parent_version,
       issued_at, expires_at, revoked
FROM axcelium.refresh_tokens
WHERE organization_id IS NOT NULL
  AND application_id IS NOT NULL
  AND token_id IS NOT NULL
  AND issued_at IS NOT NULL
  AND user_id IS NOT NULL
PRIMARY KEY (
  (organization_id, application_id, user_id),
  issued_at,
  token_id
)
WITH CLUSTERING ORDER BY (issued_at DESC);]]></string>
		</script>
